#! /bin/sh

PREFIX="${HOME:-~}"
install -v -D mulle-dockerize "${PREFIX}/bin/mulle-dockerize"
install -v -D npm/Dockerfile "${PREFIX}/share/mulle-dockerize/npm/Dockerfile"
install -v -D yo/Dockerfile "${PREFIX}/share/mulle-dockerize/yo/Dockerfile"
ln -v -s -f "${PREFIX}/bin/mulle-dockerize" "${PREFIX}/bin/npm"
ln -v -s -f "${PREFIX}/bin/mulle-dockerize" "${PREFIX}/bin/yo"

